        android:id="@+id/edSVF_DYL2X"
        android:layout_width="33dp"
        android:layout_height="35dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="26dp"
        android:textSize="8sp"
        android:ems="10"
        android:inputType="numberDecimal"
        android:text="0"
        app:layout_constraintLeft_toRightOf="@+id/SVF_DYL2X"
        app:layout_constraintTop_toTopOf="@+id/edSVF_ADC_RSEL" />

    <EditText
        android:id="@+id/edSVF_CLK_SEL"
        android:layout_width="33dp"
        android:layout_height="35dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="26dp"
        android:textSize="8sp"
        android:ems="10"
        android:inputType="numberDecimal"
        android:text="0"
        app:layout_constraintTop_toTopOf="@+id/edSVF_DYL2X"
        app:layout_constraintLeft_toRightOf="@+id/SVF_CLK_SEL" />

    <EditText
        android:id="@+id/edSVF_PERIOD"
        android:layout_width="33dp"
        android:layout_height="35dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="26dp"
        android:textSize="8sp"
        android:ems="10"
        android:inputType="numberDecimal"
        android:text="1"
        app:layout_constraintLeft_toRightOf="@+id/SVF_PERIOD"
        app:layout_constraintTop_toTopOf="@+id/edSVF_CLK_SEL" />

    <EditText
        android:id="@+id/edSVF_ISOL"
        android:layout_width="33dp"
        android:layout_height="35dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="26dp"
        android:textSize="8sp"
        android:ems="10"
        android:inputType="numberDecimal"
        android:text="1"
        app:layout_constraintLeft_toRightOf="@+id/SVF_ISOL"
        app:layout_constraintTop_toTopOf="@+id/edSVF_PERIOD" />

    <EditText
        android:id="@+id/edSVF_SL_TIME"
        android:layout_width="33dp"
        android:layout_height="35dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="26dp"
        android:textSize="8sp"
        android:ems="10"
        android:inputType="numberDecimal"
        android:text="5"
        app:layout_constraintLeft_toRightOf="@+id/SVF_SL_TIME"
        app:layout_constraintTop_toTopOf="@+id/edSVF_ISOL" />

    <EditText
        android:id="@+id/edSVF_NAVI"
        android:layout_width="33dp"
        android:layout_height="35dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="26dp"
        android:textSize="8sp"
        android:ems="10"
        android:inputType="numberDecimal"
        android:text="0"
        app:layout_constraintLeft_toRightOf="@+id/SVF_NAVI"
        app:layout_constraintTop_toTopOf="@+id/edSVF_SL_TIME" />

    <EditText
        android:id="@+id/edSVF_SCK_UT"
        android:layout_width="33dp"
        android:layout_height="35dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="26dp"
        android:textSize="8sp"
        android:ems="10"
        android:inputType="numberDecimal"
        android:text="1"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toRightOf="@+id/SVF_SCK_UT"
        app:layout_constraintTop_toTopOf="@+id/edSVF_NAVI" />

    <EditText
        android:id="@+id/edSVF_SLV_STU"
        android:layout_width="33dp"
        android:layout_height="35dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="26dp"
        android:textSize="8sp"
        android:ems="10"
        android:inputType="numberDecimal"
        android:text="0"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toRightOf="@+id/SVF_SLV_STU"
        app:layout_constraintTop_toTopOf="@+id/edSVF_SCK_UT" />

    <EditText
        android:id="@+id/edSVF_MULTIPLYING"
        android:layout_width="33dp"
        android:layout_height="35dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="26dp"
        android:textSize="8sp"
        android:ems="10"
        android:inputType="numberDecimal"
        android:text="0"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toRightOf="@+id/SVF_MULTIPLYING"
        app:layout_constraintTop_toTopOf="@+id/edSVF_SLV_STU" />

    <EditText
        android:id="@+id/edSVF_ADC_REF"
        android:layout_width="33dp"
        android:layout_height="35dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="26dp"
        android:textSize="8sp"
        android:ems="10"
        android:inputType="numberDecimal"
        android:text="100"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toRightOf="@+id/SVF_ADC_REF"
        app:layout_constraintTop_toTopOf="@+id/edSVF_MULTIPLYING" />

    <EditText
        android:id="@+id/edSVF_GPB_REG"
        android:layout_width="33dp"
        android:layout_height="35dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="26dp"
        android:textSize="8sp"
        android:ems="10"
        android:inputType="numberDecimal"
        android:text="0xf0"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toRightOf="@+id/SVF_GPB_REG"
        app:layout_constraintTop_toTopOf="@+id/edSVF_ADC_REF" />

</android.support.constraint.ConstraintLayout>
